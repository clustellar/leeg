{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "rethinkdb": {
    "db": "api",
    "servers": [
      {
        "host": "rethinkdb",
        "port": "28015"
      }
    ]
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "type": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "google": {
      "clientID": "GOOGLE_CLIENT_ID",
      "clientSecret": "GOOGLE_CLIENT_SECRET",
      "callbackURL": "http://localhost:8080/auth/google/callback",
      "successRedirect": "/",
      "scope": [
        "profile email"
      ]
    },
    "cookie": {
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": false,
      "secure": false
    }
  }
}
